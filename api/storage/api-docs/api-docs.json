{
    "openapi": "3.0.0",
    "info": {
        "title": "Driving School API",
        "description": "Description of Driving School API",
        "version": "1.0.0"
    },
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "Вход",
                "operationId": "b3f029a14629aeb7d5a705cff394e48d",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/AuthRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "No content"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                }
            }
        },
        "/logout": {
            "get": {
                "tags": [
                    "auth"
                ],
                "summary": "Выход",
                "operationId": "655d6b4d4a6499b98391f5e52a0adb3f",
                "responses": {
                    "204": {
                        "description": "No content"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                }
            }
        },
        "/sanctum/csrf-cookie": {
            "get": {
                "tags": [
                    "csrf"
                ],
                "summary": "Метод для записи csrf куки в браузер",
                "operationId": "e8dc1d9355c514153c492a692ac1f572",
                "responses": {
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/profile": {
            "get": {
                "tags": [
                    "profile"
                ],
                "summary": "Показ информации о текущем авторизованном пользователе",
                "operationId": "03cf4eee24834360232ff2cf06b3077e",
                "responses": {
                    "200": {
                        "description": "Информация о пользователе",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ProfileResource"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/slots/{slotId}/reserve": {
            "post": {
                "tags": [
                    "slots"
                ],
                "summary": "Резервирование слота для авторизованного пользователя",
                "operationId": "6fcccf19b133250d53f9d1a71ad7a83f",
                "parameters": [
                    {
                        "name": "slotId",
                        "in": "path",
                        "description": "ID слота",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Запись на вождение",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DrivingSessionResource"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Слот недоступен или с начала занятия прошло более 1 часа!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "oneOf": [
                                        {
                                            "$ref": "#/components/schemas/SlotNotAvailableException"
                                        },
                                        {
                                            "$ref": "#/components/schemas/SlotStartTimeHasAlreadyPastException"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Невозможно забронировать слот!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserNotActiveException"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Слот не найден!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotNotFoundException"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/slots/{slotId}/cancel": {
            "put": {
                "tags": [
                    "slots"
                ],
                "summary": "Отмена резервирования слота для авторизованного пользователя",
                "operationId": "cd3c8f8bbf7d26115537032597459eab",
                "parameters": [
                    {
                        "name": "slotId",
                        "in": "path",
                        "description": "ID слота",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Слот",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotResource"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Слот закрыт или с начала занятия прошло более 1 часа!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "oneOf": [
                                        {
                                            "$ref": "#/components/schemas/SlotAlreadyCompletedException"
                                        },
                                        {
                                            "$ref": "#/components/schemas/SlotStartTimeHasAlreadyPastException"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Невозможно забронировать слот!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserNotActiveException"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Слот не найден!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotNotFoundException"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/slots/{slotId}/reserve-for/{userId}": {
            "post": {
                "tags": [
                    "slots"
                ],
                "summary": "Резервирование слота для пользователя (только для админа)",
                "operationId": "97ebf8d86f84d1b2605c7be7a781331a",
                "parameters": [
                    {
                        "name": "slotId",
                        "in": "path",
                        "description": "ID слота",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "ID пользователя",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Запись на вождение",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DrivingSessionResource"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Слот недоступен!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotNotAvailableException"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Невозможно забронировать слот!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserNotActiveException"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Слот или пользователь не найден!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "oneOf": [
                                        {
                                            "$ref": "#/components/schemas/SlotNotFoundException"
                                        },
                                        {
                                            "$ref": "#/components/schemas/UserNotFoundException"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Пользователь не является студентом!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserNotStudentException"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/slots/{slotId}/cancel-for/{userId}": {
            "put": {
                "tags": [
                    "slots"
                ],
                "summary": "Отмена резервирования слота для пользователя",
                "operationId": "68303c8e2d369555bdf14bdd70e95345",
                "parameters": [
                    {
                        "name": "slotId",
                        "in": "path",
                        "description": "ID слота",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "ID пользователя",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 42
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Слот",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotResource"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Слот закрыт или с начала занятия прошло более 1 часа!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SlotAlreadyCompletedException"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Невозможно забронировать слот!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserNotActiveException"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Слот или пользователь не найден!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "oneOf": [
                                        {
                                            "$ref": "#/components/schemas/SlotNotFoundException"
                                        },
                                        {
                                            "$ref": "#/components/schemas/UserNotFoundException"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/slots": {
            "get": {
                "tags": [
                    "slots"
                ],
                "summary": "Показ всех слотов",
                "operationId": "1520c9aae027b84cd6086f13a2b06449",
                "responses": {
                    "200": {
                        "description": "Список слотов",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/SlotResource"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "DrivingSessionStatus": {
                "description": "Статус занятия\n        reservation: Забронировано,\n        completed: Закончено,\n        canceled: Отменено",
                "type": "string",
                "enum": [
                    "reservation",
                    "completed",
                    "canceled"
                ],
                "x-enum-varnames": [
                    "RESERVATION",
                    "COMPLETED",
                    "CANCELED"
                ]
            },
            "SessionCommentType": {
                "description": "Тип комментарий к занятию\n        pre: До занятия,\n        post: После занятие",
                "type": "string",
                "enum": [
                    "pre",
                    "post"
                ],
                "x-enum-varnames": [
                    "PRE",
                    "POST"
                ]
            },
            "SlotStatus": {
                "description": "Статус слота\n        available: Доступно к бронированию,\n        reservation: Забронирован,\n        completed: Закончен,\n        blocked: Заблокирован",
                "type": "string",
                "enum": [
                    "available",
                    "reservation",
                    "completed",
                    "blocked"
                ],
                "x-enum-varnames": [
                    "AVAILABLE",
                    "RESERVATION",
                    "COMPLETED",
                    "BLOCKED"
                ]
            },
            "Transmission": {
                "description": "Тип трансмиссии авто\n        mt: Механическая,\n        at: Автоматическая,\n        amt: Автоматизированная механика (робот),\n        cvt: Вариатор",
                "type": "string",
                "enum": [
                    "mt",
                    "at",
                    "amt",
                    "cvt"
                ],
                "x-enum-varnames": [
                    "MT",
                    "AT",
                    "AMT",
                    "CVT"
                ]
            },
            "UserRole": {
                "description": "Роль пользователя\n        student: Ученик,\n        instructor: Инструктор,\n        admin: Админ",
                "type": "string",
                "enum": [
                    "student",
                    "instructor",
                    "admin"
                ],
                "x-enum-varnames": [
                    "STUDENT",
                    "INSTRUCTOR",
                    "ADMIN"
                ]
            },
            "SlotAlreadyCompletedException": {
                "description": "Ответ на ошибку SlotAlreadyCompletedException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Слот уже закрыт (занятие проведено)!"
                    }
                },
                "type": "object"
            },
            "SlotNotAvailableException": {
                "description": "Ответ на ошибку SlotNotAvailableException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Невозможно забронировать слот (возможно, его уже кто-то забронировал)!"
                    }
                },
                "type": "object"
            },
            "SlotNotFoundException": {
                "description": "Ответ на ошибку SlotNotFoundException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Слот не найден!"
                    }
                },
                "type": "object"
            },
            "SlotStartTimeHasAlreadyPastException": {
                "description": "Ответ на ошибку SlotStartTimeHasAlreadyPastException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Прошло более 1 часа с начала занятия!"
                    }
                },
                "type": "object"
            },
            "UserNotActiveException": {
                "description": "Ответ на ошибку UserNotActiveException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Невозможно забронировать слот!"
                    }
                },
                "type": "object"
            },
            "UserNotFoundException": {
                "description": "Ответ на ошибку UserNotActiveException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Пользователь не найден!"
                    }
                },
                "type": "object"
            },
            "UserNotStudentException": {
                "description": "Ответ на ошибку UserNotStudentException",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Пользователь не является студентом!"
                    }
                },
                "type": "object"
            },
            "AuthRequest": {
                "properties": {
                    "pernumber": {
                        "type": "string",
                        "example": "100000000"
                    },
                    "password": {
                        "type": "string",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "CarResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "brand": {
                        "type": "string",
                        "example": "Lada"
                    },
                    "model": {
                        "type": "string",
                        "example": "Kalina"
                    },
                    "color": {
                        "type": "string",
                        "example": "Портвейн"
                    },
                    "state_number": {
                        "type": "string",
                        "example": "А100А099"
                    },
                    "transmission": {
                        "$ref": "#/components/schemas/Transmission"
                    },
                    "is_active": {
                        "type": "boolean",
                        "example": true
                    },
                    "instructor_id": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "CarSlotResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "brand": {
                        "type": "string",
                        "example": "Lada"
                    },
                    "model": {
                        "type": "string",
                        "example": "Kalina"
                    },
                    "color": {
                        "type": "string",
                        "example": "Портвейн"
                    },
                    "state_number": {
                        "type": "string",
                        "example": "А100А099"
                    },
                    "transmission": {
                        "$ref": "#/components/schemas/Transmission"
                    }
                },
                "type": "object"
            },
            "DrivingSessionResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "score": {
                        "type": "integer",
                        "example": "5"
                    },
                    "status": {
                        "$ref": "#components/schemas/SlotStatus"
                    },
                    "slot_id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "student_id": {
                        "type": "integer",
                        "example": "1"
                    }
                },
                "type": "object"
            },
            "InstructorResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "bio": {
                        "type": "string",
                        "example": "О себе"
                    },
                    "experience": {
                        "type": "integer",
                        "example": 1
                    },
                    "rating": {
                        "type": "number",
                        "format": "float",
                        "example": 3
                    },
                    "is_active": {
                        "type": "boolean",
                        "example": true
                    },
                    "active_car": {
                        "$ref": "#/components/schemas/CarResource"
                    }
                },
                "type": "object"
            },
            "InstructorSlotResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "bio": {
                        "type": "string",
                        "example": "О себе"
                    },
                    "experience": {
                        "type": "integer",
                        "example": 1
                    },
                    "rating": {
                        "type": "number",
                        "format": "float",
                        "example": 3
                    }
                },
                "type": "object"
            },
            "ProfileResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "pernumber": {
                        "type": "number",
                        "example": "100000000"
                    },
                    "phone": {
                        "type": "number",
                        "example": "79999999999"
                    },
                    "name": {
                        "type": "string",
                        "example": "Name Name"
                    },
                    "role": {
                        "$ref": "#/components/schemas/UserRole"
                    },
                    "is_active": {
                        "type": "boolean",
                        "example": true
                    }
                },
                "type": "object"
            },
            "SlotResource": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "start_time": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-01-01 21:00:00"
                    },
                    "status": {
                        "$ref": "#/components/schemas/SlotStatus"
                    },
                    "instructor": {
                        "$ref": "#/components/schemas/InstructorSlotResource"
                    },
                    "car": {
                        "$ref": "#/components/schemas/CarSlotResource"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "auth",
            "description": "auth"
        },
        {
            "name": "csrf",
